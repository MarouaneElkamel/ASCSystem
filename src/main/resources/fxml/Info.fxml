<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="320.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="InfoController">
   <children>
      <VBox prefHeight="250.0" prefWidth="300.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <StackPane>
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Please enter your information below" />
               </children>
            </StackPane>
            <AnchorPane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <GridPane prefHeight="400.0" prefWidth="200.0" vgap="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Enter your age" />
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Are you drunk?" GridPane.rowIndex="3" />
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Select the weather condition" GridPane.rowIndex="6" />
                        <StackPane GridPane.columnIndex="1">
                           <children>
                              <ChoiceBox fx:id="AgeChoice" prefWidth="150.0" />
                           </children>
                        </StackPane>
                        <StackPane GridPane.columnIndex="1" GridPane.rowIndex="3">
                           <children>
                              <ChoiceBox fx:id="drunkChoice" prefWidth="150.0" />
                           </children>
                        </StackPane>
                        <StackPane GridPane.columnIndex="1" GridPane.rowIndex="6">
                           <children>
                              <ChoiceBox fx:id="weatherChoice" prefWidth="150.0" />
                           </children>
                        </StackPane>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Are you tired?" GridPane.rowIndex="1" />
                        <StackPane GridPane.columnIndex="1" GridPane.rowIndex="1">
                           <children>
                              <ChoiceBox fx:id="TiredChoice" prefWidth="150.0" />
                           </children>
                        </StackPane>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Is the car rented?" GridPane.rowIndex="5" />
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Do you consider yourself aggressive?" GridPane.rowIndex="2" />
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Did you put your seat belt ?" GridPane.rowIndex="4" />
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Select the road condition" GridPane.rowIndex="7" />
                        <StackPane GridPane.columnIndex="1" GridPane.rowIndex="2">
                           <children>
                              <ChoiceBox fx:id="AggressiveChoice" prefWidth="150.0" />
                           </children>
                        </StackPane>
                        <StackPane GridPane.columnIndex="1" GridPane.rowIndex="5">
                           <children>
                              <ChoiceBox fx:id="rentedChoice" prefWidth="150.0" />
                           </children>
                        </StackPane>
                        <StackPane prefHeight="137.0" prefWidth="236.0" GridPane.columnIndex="1" GridPane.rowIndex="7">
                           <children>
                              <ChoiceBox fx:id="roadChoice" prefWidth="150.0" />
                           </children>
                        </StackPane>
                        <StackPane GridPane.columnIndex="1" GridPane.rowIndex="4">
                           <children>
                              <ChoiceBox fx:id="securityChoice" prefWidth="150.0" />
                           </children>
                        </StackPane>
                     </children>
                  </GridPane>
               </children>
            </AnchorPane>
            <StackPane>
               <children>
                  <Button fx:id="submit" mnemonicParsing="false" onAction="#onSubmit" text="Submit" />
               </children>
            </StackPane>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
